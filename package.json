{
  "name": "s-cyber-sage",
  "version": "0.1.0",
  "description": "CDK app for deploying serverless cyber sage",
  "private": true,
  "scripts": {
    "build": "cargo lambda build --release --manifest-path s-cybersage-rs/Cargo.toml",
    "lint": "eslint .",
    "format": "prettier --write .",
    "predeploy": "npm run build",
    "deploy": "bash -c 'cdk deploy \"$@\"' --",
    "register-commands": "tsx commands/register-commands.ts"
  },
  "dependencies": {
    "@aws-cdk/aws-lambda-python-alpha": "^2.215.0-alpha.0",
    "@discordjs/rest": "^0.4.1",
    "aws-cdk-lib": "^2.239.0",
    "constructs": "^10.5.1",
    "discord-api-types": "^0.38.25",
    "discord.js": "^13.17.1",
    "dotenv": "^17.2.2",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "cdk-nag": "^2.37.13",
    "prettier": "^3.6.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.2"
  }
}
